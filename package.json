{
  "name": "backend-stage-two",
  "version": "1.0.0",
  "description": "backend-stage-two",
  "private": true,
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.{js,ts}",
    "prettier:fix": "prettier --write **/*.{js,ts}",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "node .esbuild/esbuild.config.cjs",
    "build:watch": "node .esbuild/esbuild-watchmode.cjs",
    "start": "node build/index.js",
    "start:watch": "nodemon -q build/index.js",
    "predev": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vinejs/vine": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "mongoose": "^7.4.3",
    "typescript": "^5.1.6",
    "validator": "^13.11.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.1",
    "@types/validator": "^13.11.1",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.2",
    "esbuild-node-externals": "^1.8.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1"
  }
}
